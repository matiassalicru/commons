    "build-storybook": "build-storybook",
    "eslint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "eslint:fix": "eslint --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "prepare": "husky install",
    "tsc": "tsc",
    "run-tests": "npm run test; npm run eslint"
  },
