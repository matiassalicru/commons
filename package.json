    "np-publish": "npm run build; npm run release;",
    "build-dev": "react-scripts build",
    "start": "react-scripts start",
    "build": "npm run clean; babel src --out-dir lib --copy-files src/ --extensions \".ts,.tsx,.js\" --source-maps inline ",
